<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.quantify.liferay">

	<namespace>quantify</namespace>

	<entity local-service="true" name="Workout" remote-service="true" uuid="true">

		<!-- PK fields -->
		<column name="workoutId" primary="true" type="long" />
		<!-- Group instance -->
		<column name="groupId" type="long" />
		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->
		<column name="description" type="String" />
		<column name="sportId" type="long" />
		<column name="date" type="Date" />

	</entity>

	<entity local-service="true" name="Sport" remote-service="true" uuid="true">

		<!-- PK fields -->
		<column name="sportId" primary="true" type="long" />
		<!-- Group instance -->
		<column name="groupId" type="long" />
		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->
		<column name="description" type="String" />

	</entity>

	<entity local-service="true" name="Zone" remote-service="true" uuid="true">

		<!-- PK fields -->
		<column name="zoneId" primary="true" type="long" />
		<!-- Group instance -->
		<column name="groupId" type="long" />
		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->
		<column name="code" type="String" />
		<column name="description" type="String" />

	</entity>

	<entity local-service="true" name="UserZone" remote-service="true" uuid="true">

		<!-- PK fields -->
		<column name="userZoneId" primary="true" type="long" />

		<!-- Audit fields -->
		<column name="userId" type="long" />
		<column name="zoneId" type="long" />
		<column name="from" type="long" />
		<column name="to" type="long" />

	</entity>

	<entity local-service="true" name="Athlete" remote-service="true" uuid="true">

		<!-- PK fields -->
		<column name="athleteId" primary="true" type="long" />

		<!-- Other fields -->
		<column name="userId" type="long" />
		<column name="date" type="Date" />

	</entity>


</service-builder>


